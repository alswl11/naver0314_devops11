package day0319;

public class Ex3_Munje {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		 *  xy + yx = 121 나오도록 하는 한 자릿수 x,y 구하기  
		 *  29+92 = 121
		 */
		
		int x=0, y = 0, num = 0;
		
		for(int i=1; i<10; i++) {
			for (int j = 1; i<10; j++) {
				if ((10*i+j) +(10*j + i) == 121) {
					i = x;
					j = y;
					System.out.println("x= " + x +  "y = " +y);
				}
			}
		}

	}

}
