package day0321;

public class Ex8_Array2Cha {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [][] arr;
		arr = new int [2][3]; // 2행 3열 
		System.out.println("행 갯수 : "+arr.length);
		System.out.println("0번행의 열 갯수 : "+ arr[0].length);
		System.out.println();
	}

}
