package day0319;

public class Ex1_Munje {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		 *  1-100 숫자 중 소수를 찾는 문제 
		 *  다중 for문 사
		 */

		int count=0;
		
		for(int i = 2; i<=100; i++) {
			for (int j=2; j<=i; j++) {
				if(i%j == 0)
					count++;
			}	
		}
		System.out.println(count+"개");
	}

}
