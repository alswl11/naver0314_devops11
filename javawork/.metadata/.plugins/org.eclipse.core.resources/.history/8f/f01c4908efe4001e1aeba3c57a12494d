package day0318;

public class Ex9_For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i = 1; i<=10; i++) 
			System.out.printf("%4d", i); // 4칸 단위로 출력 
		System.out.println();
		
		for(int i = 10; i>=1; i--) 
			System.out.printf("%4d", i); // 4칸 단위로 출력 
		System.out.println();

	}

}
