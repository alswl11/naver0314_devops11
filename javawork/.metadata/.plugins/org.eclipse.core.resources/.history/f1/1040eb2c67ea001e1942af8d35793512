package day0325;

// 상속 
class AA{ // AA : 부모 
	AA(){
		System.out.println("AA의 디폴트 생성자");
	}
	
}

class BB extends AA{  // BB : 자식 
	BB(){
		System.out.println("BB의 디폴트 생성자");
	}
}

public class Ex4_Inheri {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		BB b = new BB();
	}

}
