package day0322;

public class Ex5_callBy {

	public static void changeNum(int n) {
		n = 10;
	}
	
	public static void chargeArray(int [] a) {
		a[1] = 200;
	}
	
	public static void changeHome(String home) {
		home = "제주도";
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 50;
		changeNum(n); // 값만 전달, call by value(기본형은 모두 해당)
		System.out.println("n = " + n); // 50
		
		int [] arr = {3,5,7}; // 무조건 배열은 객체로 인식 
		chargeArray(arr); // 주소가 전달되었고, call by reference 
		for(int a : arr) {
			System.out.printf("%4d ", a); // 3 200 7
		}
		System.out.println();
		
		
		String home = "서울";
		changeHome(home); // call by value : 주소가 아닌 값이 전달 
		System.out.println(home); // 여전히 "서울"이라고 출력, 주소가 아닌 주소만 전달되기 때문에 
		
	}

}
