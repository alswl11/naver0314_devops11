package day0321;

public class Ex2_ArraySort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 기준 i =  0 ~ n-1
		// 비교 j = i+1 ~ n

		// 두 값을 바꿀 경우 
		int a = 10, b = 20;
		int temp; // 임시변수  
		System.out.println("a = " + a +", b = " +b);

		// 두 값을 바꾸기  
		temp = a;
		a =b;
		b = temp;
		System.out.println("a = " + a + ", b = " +b);

	}

}
